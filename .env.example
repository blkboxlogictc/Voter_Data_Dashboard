# Environment Variables for Voter Data Dashboard
# Copy this file to .env and fill in your actual values

# Mapbox Configuration (Optional - for enhanced maps)
# Get your token from: https://account.mapbox.com/access-tokens/
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# Census API Configuration (Optional - uses default if not provided)
# Get your key from: https://api.census.gov/data/key_signup.html
VITE_CENSUS_API_KEY=f0b106758e077c8e0d3dbd117d83cce29ffe0f11

# Database Configuration (Optional - for advanced features)
# Only needed if using database features
DATABASE_URL=your_database_url_here

# Census API Key for server-side functions (Optional)
# Same as VITE_CENSUS_API_KEY but for backend functions
CENSUS_API_KEY=f0b106758e077c8e0d3dbd117d83cce29ffe0f11

# Node Environment (automatically set by Netlify)
NODE_ENV=production